{
    "name": "sTab",
    "description": "Kill and Revive Chrome Tab Process",
    "version": "1.0.1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", "processes"
    ],
    "browser_action": {
        "default_icon": {
            "19": "icons/19.png",
            "38": "icons/38.png"
        },
        "default_title": "Kill"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "manifest_version": 2
}
